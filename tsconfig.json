    "target": "ES5",
    "module": "CommonJS",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node",
    ],
    "allowJs": true,
    "outDir": "lib",
    
  },
  "typeRoots": [
    "./node_modules/@types",
  ],
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "lib",
  ],
}